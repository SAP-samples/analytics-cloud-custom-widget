{
  "eula": "",
  "vendor": "SAP",
  "license": "",
  "id": "com.sap.sac.sample.echarts.sankey",
  "icon": "data:image/png;base64,",
  "version": "1.0.0",
  "supportsMobile": true,
  "supportsLinkedAnalysisFilterOnSelection": true,
  "name": "EChart Sankey Chart: SAP SE",
  "newInstancePrefix": "EChartsSankeyStyling",
  "description": "A Sample Sankey Chart with Styling Panel",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-sac-sample-echarts-sankey",
      "url": "/mainS.js",
      "integrity": "",
      "ignoreIntegrity": true
    },
    {
      "kind": "styling",
      "tag": "com-sap-sac-sample-echarts-sankey-styling",
      "url": "/StylingS.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "width": {
      "type": "integer",
      "default": 600
    },
    "height": {
      "type": "integer",
      "default": 320
    },
    "depth0Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#fbb4ae", "lineOpacity": 0.6}
    },
    "depth1Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#b3cde3", "lineOpacity": 0.4}
    },
    "depth2Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#ccebc5", "lineOpacity": 0.2}
    },
    "depth3Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#decbe4", "lineOpacity": 0.1}
    }
  },
  "events": {},
  "methods": {
		"getDataSource": {
			"parameters": [],
			"body": "return this.dataBindings.getDataBinding('dataBinding').getDataSource();",
			"returnType": "DataSource"
		}
  },
  "types": {
    "stylingSetting": {
      "description": "type for styling settings",
      "properties": [
        {
          "name": "itemColor",
          "type": "string",
          "description": "item cloor"
        },
        {
          "name": "lineOpacity",
          "type": "number",
          "description": "line opacity"
        }
      ]
    }
  },
  "dataBindings": {
    "dataBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "Measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}