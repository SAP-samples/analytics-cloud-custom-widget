{
  "eula": "",
  "vendor": "SAP",
  "license": "",
  "id": "com.sap.sac.exercise.username",
  "version": "1.0.0",
  "name": "Guided Custom Widget Exercise",
  "icon": "data:image/png;base64,",
  "newInstancePrefix": "ExerciseCustomWidget",
  "description": "A Guided Custom Widget Exercise",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-sac-exercise-username-main",
      "url": "/main.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "width": {
      "type": "integer",
      "default": 600
    },
    "height": {
      "type": "integer",
      "default": 420
    },
    "seriesType": {
      "type": "string",
      "default": "line",
      "description": "Series type"
    }
  },
  "methods": {
    "setSeriesType": {
      "description": "Set series type",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "Series type"
        }
      ]
    }
  },
  "events": {
    "onClick": {
      "description": "Dispatch when user click a data point on eChart"
    }
  },
  "dataBindings": {
    "dataBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "Measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}
